{
  "build": {
    "command": "npm run build:web",
    "output": "web-build"
  },
  "start": {
    "command": "npm start"
  },
  "env": {
    "EXPO_PUBLIC_API_URL": {
      "default": "https://jctop.zeabur.app/api/v1"
    },
    "NODE_ENV": {
      "default": "production"
    },
    "PORT": {
      "default": "8080"
    }
  }
}